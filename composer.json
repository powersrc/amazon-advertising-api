{
  "name": "powersrc/amazon-advertising-api",
  "type": "library",
  "description": "Amazon Advertising API Client Library",
  "keywords": [
    "framework",
    "amazon",
    "advertising api",
    "client"
  ],
  "homepage": "https://powersrc.net/",
  "license": "MIT",
  "authors": [
    {
      "name": "Jason Hebert",
      "email": "jason.hebert@powersrc.net"
    },
    {
      "name": "Matthew Holder",
      "email": "matthew.holder@powersrc.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "PowerSrc\\AmazonAdvertisingApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PowerSrc\\AmazonAdvertisingApi\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-zlib": "*",
    "guzzlehttp/guzzle": "^7.0.1",
    "myclabs/php-enum": "^1.6",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16.0",
    "phpro/grumphp": "^0.15.0",
    "phpunit/phpunit": "~7.0",
    "sensiolabs/security-checker": "^5.0"
  },
  "scripts": {
    "check-style": "grumphp run",
    "fix-style": [
      "php-cs-fixer fix -v"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
